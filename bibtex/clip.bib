@techreport{ddm-anal,
  author = {C. Carreras and M. Hermenegildo},
  title = {{T}owards an {A}nalytical {M}odel of the {D}ata {D}iffusion
                  {M}achine},
  institution = {UPM (PEPMA ESPRIT Project technical report)},
  year = {1990},
  projects = {PEPMA},
  butopics = {simul,arch},
  butype = {TechReport},
  Abstract = {This paper presents an approach for developing an
                  analytical model of the Data Diffusion Machine
                  (DDM). The DDM can briefly be described as a
                  scalable shared virtual memory multiprocessor
                  organized as a multi-level hierarchy of buses and
                  controllers. The main characteristic of the DDM is
                  that the location of a datum is completely decoupled
                  from its virtual address so data migrates
                  automatically to where is needed. The objective of
                  such a model is to allow an analysis to determine
                  the optimum DDM topology for a given number of
                  processors and a given number of levels. The paper
                  considers different elements of the architecture and
                  discusses simplifying assumptions and their
                  validity. A general model is presented based on
                  these assumptions.}
}

