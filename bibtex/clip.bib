@inproceedings{granularity,
  author = {S. K.~Debray and N.-W.~Lin and M.~Hermenegildo},
  title = {{T}ask {G}ranularity {A}nalysis in {L}ogic {P}rograms},
  booktitle = {Proc. of the 1990 {ACM} Conf. on Programming Language
                  Design and Implementation},
  pages = {174--188},
  year = {1990},
  month = {June},
  publisher = {{ACM} Press},
  butopics = {gran,autop,anal},
  buconfj = {pldi},
  butype = {InProceedings},
  Abstract = {While logic programming languages offer a great deal of
                  scope for parallelism, there is usually some
                  overhead associated with the execution of goals in
                  parallel because of the work involved in task
                  creation and scheduling. In practice, therefore, the
                  ``granularity'' of a goal, i.e. an estimate of the
                  work available under it, should be taken into
                  account when deciding whether or not to execute a
                  goal concurrently as a separate task. This paper
                  describes a method for estimating the granularity of
                  a goal at compile time. The runtime overhead
                  associated with our approach is usually quite small,
                  and the performance improvements resulting from the
                  incorporation of grainsize control can be quite
                  good.This is shown by means of experimental
                  results.},
  paper_presentation_city ={Boston},
  paper_presentation_country ={USA}
}

