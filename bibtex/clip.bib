@techreport{pl2akl-results-fimtr,
  author = {F. Bueno and M. Hermenegildo},
  title = {{R}esults on {A}utomatic {T}ranslation from {P}rolog to the
                  {A}ndorra {K}ernel {L}anguage},
  institution = {T.U. of Madrid (UPM)},
  year = {1991},
  month = {September},
  address = {Facultad Informatica UPM, 28660-Boadilla del Monte,
                  Madrid-Spain},
  number = {CLIP2/91.0},
  type = {Technical Report},
  note = {theory in technical report, ``Towards an Automatic
                  Translation Algorithm from Prolog to the Andorra
                  Kernel Language''},
  butopics = {autop,parmod,conc},
  butype = {TechReport},
  Abstract = {This report presents some results on the timing of a
                  number of benchmarks executed in Prolog and in
                  AKL. The AKL versions of the programs have been
                  obtained with an automatic compile-time translator,
                  in the intention of not only comparing both
                  languages performance, but also testing the
                  compile-time tool developed by us. Benchmarks have
                  been chosen so that performance results are taken
                  for different programming paradigms, and different
                  translation issues are taken into account. It is
                  shown a variable performance improvement for AKL
                  vs. Prolog whenever determinism is significant in
                  the program, and also that translation suffices in
                  most cases with state-of-art analysis technology.}
}


